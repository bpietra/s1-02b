import { StatusBar } from 'expo-status-bar';
import { StyleSheet, Text, View } from 'react-native';
export default function App() {
  return (
    <View >
    <Text style={styles.container}>A Dona Aranha</Text>
      <Text>Dona Aranha
subiu pela parede
veio a chuva forte e a derrubou
já passou a chuva
e o sol já vem surgindo
e a dona aranha
continua a subir
ela é teimosa,
desobediente.
sobe, sobe, sobe
nunca está contente!!</Text>
      <StatusBar style="auto" />
    </View>
  );
}
const styles = StyleSheet.create({
  container: {
    flex: 1,
    backgroundColor: '#fff',
    alignItems: 'left',
    justifyContent: 'top',
    fontSize: '50px',
  },
});
